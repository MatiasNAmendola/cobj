#!./co

try {
    print "try-catch: try";
    throw "E";
} catch ("E") {
    print "try-catch: catch";
}

try {
    print "try-finally: try";
} finally {
    print "try-finally: finally";
}

try {
    print "try-catch-finally: try";
    throw "E";
} catch ("E") {
    print "try-catch-finally: catch";
} finally {
    print "try-catch-finally: finally";
}
